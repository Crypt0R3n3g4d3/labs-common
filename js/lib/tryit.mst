  <code>
    <a class="exec has-tooltip ga-track" data-toggle="tooltip" title="" href="{{url}}" data-original-title="View the output of this query" data-container="body" data-tracking-category="tryit" data-tracking-label="run-query" data-tracking-value="{{display_url}}">{{display_url}}</a>
